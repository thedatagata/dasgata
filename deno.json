{
  "tasks": {
    "start": "deno run -A --unstable-kv --watch=static/,routes/,components/,islands/ dev.ts",
    "build": "deno run -A --unstable-kv dev.ts build",
    "preview": "deno run -A --unstable-kv main.ts"
  },
  "imports": {
    "$fresh/": "https://deno.land/x/fresh@1.7.3/",
    "@mlc-ai/web-llm": "npm:@mlc-ai/web-llm@^0.2.72",
    "@launchdarkly/observability-node": "npm:@launchdarkly/observability-node@^0.3.1",
    "launchdarkly-node-client-sdk": "npm:launchdarkly-node-client-sdk@^3.3.0",
    "preact": "https://esm.sh/preact@10.22.0",
    "preact/": "https://esm.sh/preact@10.22.0/",
    "preact/jsx-runtime": "https://esm.sh/preact@10.22.0/jsx-runtime",
    "preact-render-to-string": "https://esm.sh/*preact-render-to-string@6.2.2",
    "@preact/signals": "https://esm.sh/*@preact/signals@1.2.2",
    "@preact/signals-core": "https://esm.sh/*@preact/signals-core@1.5.1",
    "sharp": "npm:sharp@^0.34.5",
    "twind": "https://esm.sh/twind@0.16.19",
    "twind/": "https://esm.sh/twind@0.16.19/",
    "@std/": "https://deno.land/std@0.213.0/",
    "$std/": "https://deno.land/std@0.213.0/",
    "@deno/kv-oauth": "jsr:@deno/kv-oauth",
    "@twind/core": "https://esm.sh/@twind/core@1.1.3",
    "@twind/preset-tailwind": "https://esm.sh/@twind/preset-tailwind@1.1.4/",
    "@twind/preset-autoprefix": "https://esm.sh/@twind/preset-autoprefix@1.0.7/",
    "@observablehq/plot": "npm:@observablehq/plot@0.6",
    "@launchdarkly/node-server-sdk": "npm:@launchdarkly/node-server-sdk@^9.10.2"
  },
  "nodeModulesDir": "auto",
  "compilerOptions": { "jsx": "react-jsx", "jsxImportSource": "preact" },
  "fmt": { "indentWidth": 2, "lineWidth": 100, "semiColons": true },
  "unstable": ["kv"],
  "lint": { "rules": { "tags": ["recommended", "fresh"] } },
  "exclude": ["**/_fresh/*"]
}
